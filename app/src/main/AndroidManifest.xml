<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.zezutom.twiitalyser" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.crashlytics.ApiKey"
            android:value="fb2ad41b372ef9d5410c6f2d4173bed516e63214" />

        <activity
            android:name=".EmbeddedTweetsActivity"
            android:label="@string/title_activity_tweet" >
        </activity>

        <activity
            android:name=".TweetListActivity"
            android:label="@string/title_activity_tweet_list" >
        </activity>
    </application>

</manifest>
